{
    "name": "milan-miscevic/cv",
    "description": "My CV",
    "license": "MIT",
    "type": "project",
    "keywords": [
        "cv",
        "milan-miscevic",
        "resume"
    ],
    "authors": [
        {
            "name": "Milan Miščević",
            "email": "milan.miscevic@gmail.com"
        }
    ],
    "require": {
        "php": "^8.1"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.11.0",
        "infection/infection": "^0.26.14",
        "phpstan/phpstan": "^1.8.5",
        "phpstan/phpstan-strict-rules": "^1.4",
        "phpunit/phpunit": "^9.5.24",
        "vimeo/psalm": "^4.27.0"
    },
    "autoload": {
        "psr-4": {
            "Mmm\\Cv\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Mmm\\Cv\\Tests\\": "tests/"
        }
    },
    "config": {
        "allow-plugins": {
            "infection/extension-installer": false
        },
        "sort-packages": true
    }
}
